<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/photoImage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="0dp"
        android:contentDescription="@string/photo" />

    <TextView
        android:id="@+id/datetimeCText1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="85dp"
        android:layout_marginTop="10dp"
        android:layout_alignParentStart="true"
        android:text="@string/date_time"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#0016b5"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/datetimeCText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="87dp"
        android:layout_marginTop="12dp"
        android:layout_alignParentStart="true"
        android:text="@string/date_time"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#0016b5"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/datetimeCText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="86dp"
        android:layout_marginTop="11dp"
        android:layout_alignParentStart="true"
        android:text="@string/date_time"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#0016b5"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/datetimeCText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="86dp"
        android:layout_marginTop="11dp"
        android:layout_alignParentStart="true"
        android:text="@string/date_time"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#fffb00"
        android:textSize="16sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/placeCText1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0sp"
        android:layout_marginBottom="45dp"
        android:text="@string/txtPlace"
        android:textAlignment="center"
        android:textColor="#0016b5"
        android:textSize="28sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/placeCText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4sp"
        android:layout_marginBottom="49dp"
        android:text="@string/txtPlace"
        android:textAlignment="center"
        android:textColor="#0016b5"
        android:textSize="28sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/placeCText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3sp"
        android:layout_marginBottom="48dp"
        android:text="@string/txtPlace"
        android:textAlignment="center"
        android:textColor="#0016b5"
        android:textSize="28sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/placeCText4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2sp"
        android:layout_marginBottom="47dp"
        android:text="@string/txtPlace"
        android:textAlignment="center"
        android:textColor="#fffb00"
        android:textSize="28sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/address1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0sp"
        android:layout_marginBottom="25dp"
        android:layout_alignParentBottom="true"
        android:text="@string/txtAddress"
        android:textAlignment="center"
        android:textColor="#0016b5"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/address2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="2sp"
        android:layout_marginBottom="27dp"
        android:layout_alignParentBottom="true"
        android:text="@string/txtAddress"
        android:textAlignment="center"
        android:textColor="#0016b5"
        android:textSize="14sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/address3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginBottom="26dp"
        android:layout_alignParentBottom="true"
        android:text="@string/txtAddress"
        android:textAlignment="center"
        android:textColor="#fffb00"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/GPSText1"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="4dp"
        android:text="@string/txtPlace"
        android:textColor="#151aa5"
        android:gravity="center"
        android:textSize="12sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/GPSText2"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"
        android:text="@string/txtPlace"
        android:gravity="center"
        android:textColor="#fffb00"
        android:textSize="12sp"
        android:textStyle="bold" />


    <ImageView
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="40dp"
        android:contentDescription="@string/todo"
        android:padding="0dp"
        android:alpha="0.2"
        android:src="@raw/signature_yellow" />

</RelativeLayout>

